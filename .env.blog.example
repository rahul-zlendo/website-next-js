# WordPress Blog Configuration
# Copy this to .env.local and update values

# WordPress REST API Base URL (where WordPress is installed)
WP_BASE_URL=https://zlendorealty.com/blog

# ISR Revalidation time in seconds (3600 = 1 hour)
WP_REVALIDATE_SECONDS=3600

# Help Center WordPress (BetterDocs)
HC_BASE_URL=https://helpcenter.zlendorealty.com
HC_REVALIDATE_SECONDS=3600

# Site URL for canonical links and sitemaps
NEXT_PUBLIC_SITE_URL=https://zlendorealty.com
